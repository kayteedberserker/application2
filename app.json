{
  "expo": {
    "name": "Oreblogda",
    "slug": "oreblogda",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/myicon.png",
    "scheme": "oreblogda",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaytee.oreblogda",
      "associatedDomains": [
        "applinks:oreblogda.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/myicon.png",
        "backgroundImage": "./assets/images/myicon.png"
      },
      "package": "com.kaytee.oreblogda",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "oreblogda.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "oreblogda"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8021671365048667~3586113443"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/myicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/myicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8021671365048667~3586113443",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511",
          "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "dependencies": [
              "com.google.ads.mediation:applovin:13.5.1.0"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b6b7afd1-028a-425e-a84e-6a2af706a736"
      }
    },
    "runtimeVersion": "v1_global",
    "updates": {
      "url": "https://u.expo.dev/b6b7afd1-028a-425e-a84e-6a2af706a736",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 10000 
    },
    "owner": "oreblogda"
  }
}
